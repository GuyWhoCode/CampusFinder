{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-template-curly-in-string":"error",
        "block-scoped-var":"error",
        "eqeqeq":"warn",
        "no-else-return":"warn"
    }
}